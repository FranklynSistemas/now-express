{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/users",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/socket.io",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/socket/socket.io",
      "dest": "/index.js",
      "methods": ["GET"]
    },
  ]
}
